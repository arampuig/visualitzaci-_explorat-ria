    "carr_grp = df.groupby(['UniqueCarrier'])\n",
    "\n",
    "x = carr_grp[['ArrDelay', 'DepDelay']].mean()\n",
    "x = x.sort_values(by=['ArrDelay'], ascending=False)\n",
    "\n",
    "labels = 'Arrival Delay', 'Departure Delay'\n",
    "\n",
    "arr_dep_avg = plt.plot(x, marker = 'o', ms = 10)\n",
    "\n",
    "plt.title('Average Arrival/ Departure Delay by Carrier')\n",
    "plt.ylabel('Minutes')\n",
    "\n",
    "plt.legend(['Arrival Delay', 'Departure Delay'])\n",
    "\n",
    "# plt.show()\n",
    "plt.savefig('5-1.jpg')"
   ]
  },
